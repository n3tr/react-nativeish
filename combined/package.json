{
  "name": "react-nativish",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/react-native": "^3.1.9",
    "jest-expo": "~18.0.0",
    "react-native-scripts": "0.0.50",
    "react-test-renderer": "16.0.0-alpha.12"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "install:web": "yarn add react@15.6 react-dom@15.6",
    "install:native": "yarn add react@16.0.0-alpha.12 react-dom@16.0.0-alpha.12",
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "yarn install:native && jest -c .jest-native.json && yarn test:web",
    "start:web": "react-scripts start",
    "build:web": "react-scripts build",
    "test:web": "yarn install:web && CI=true react-scripts test --env=jsdom ./src",
    "storybook": "yarn install:native && storybook start -p 7007 -c storybook",
    "storybook:web": "yarn install:web &&  start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "@storybook/react": "^3.1.9",
    "expo": "^18.0.3",
    "react": "16.0.0-alpha.12",
    "react-dom": "16.0.0-alpha.12",
    "react-native": "^0.45.1",
    "react-native-web": "^0.0.113",
    "react-scripts": "1.0.10"
  }
}
