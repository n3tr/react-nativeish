{
  "name": "react-nativish",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/react-native": "^3.1.9",
    "jest-expo": "~18.0.0",
    "react-native-scripts": "0.0.50",
    "react-test-renderer": "15.6"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "lint": "eslint src",
    "install:web": "node ./scripts/install-web.js",
    "install:mobile": "node ./scripts/install-mobile.js",
    "start": "yarn install:web && react-native-scripts start",
    "eject": "yarn install:web && react-native-scripts eject",
    "android": "yarn install:mobile && react-native-scripts android",
    "ios": "yarn install:mobile && react-native-scripts ios",
    "test": "yarn lint && yarn test:mobile && CI=true yarn test:web && yarn install:mobile",
    "test:mobile": "yarn install:mobile && jest -c .jest-native.json",
    "start:web": "yarn install:web && react-scripts start",
    "build:web": "yarn install:web && react-scripts build",
    "test:web": "yarn install:web && react-scripts test --env=jsdom ./src",
    "storybook": "yarn install:mobile && storybook start -p 7007 -c storybook",
    "storybook:web": "yarn install:web &&  start-storybook -p 9001 -c .storybook"
  },
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "@storybook/react": "^3.1.9",
    "chalk": "^2.0.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-flow": "^1.0.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "expo": "^18.0.3",
    "jest-enzyme": "^3.6.1",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.5.10",
    "raf": "^3.3.2",
    "react": "15.6",
    "react-dom": "15.6",
    "react-native": "^0.45.1",
    "react-native-web": "^0.0.113",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "react-scripts": "1.0.10",
    "redux": "^3.7.2"
  }
}
